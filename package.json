{
  "name": "Quba",
  "version": "1.4.1",
  "description": "Viewer",
  "author": "Quba",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && electron-builder build --win --publish never",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "pack": "npm run build && electron-builder build --publish=never",
    "start": "vue-cli-service build && electron .",
    "dist": "electron-builder",
    "publish": "electron-builder build --win --publish always"
  },
  "main": "app/main.js",
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "3.24.1",
    "electron-in-page-search": "^1.3.2",
    "electron-is-dev": "2.0.0",
    "electron-localshortcut": "3.2.1",
    "electron-store": "8.0.1",
    "electron-updater": "5.0.1",
    "fast-xml-parser": "3.19.0",
    "i18next": "21.8.5",
    "i18next-fs-backend": "1.1.4",
    "ini": "^5.0.0",
    "jquery": "^3.7.1",
    "jsdom": "^23.0.1",
    "node-localstorage": "^2.2.1",
    "pdfjs-dist": "2.9.359",
    "saxon-js": "2.4.0",
    "vue": "3.2.36",
    "vue-i18n": "9.14.1",
    "vue-sweetalert2": "^5.0.5",
    "vue-tabs-chrome": "0.10.0",
    "vue3-tabs-chrome": "0.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "4.2.0",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/compiler-sfc": "3.2.36",
    "electron": "33.0.1",
    "electron-builder": "25.1.8",
    "vue-cli-plugin-i18n": "2.3.1"
  },
  "build": {
    "appId": "org.quba-viewer.viewer",
    "mac": {
      "identity": "Jochen Staerk (W92KCRC9LM)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "notarize": true,
      "category": "Document",
      "icon": "./assets/img/logo_small_icon_only.png",
      "target": {
        "target": "dmg",
        "arch": [
          "universal"
        ]
      }
    },
    "win": {
      "icon": "./assets/img/logo_small_icon_only.png",
      "target": [
        "nsis","msi"
      ]
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "**/*",
      "dist/**/*",
      "!*.map",
      "!package.json",
      "!package-lock.json"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "ZUGFeRD",
        "repo": "quba-viewer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZUGFeRD/quba-viewer.git"
  },
  "keywords": [],
  "license": "APL"
}
